   "source": [
    "try:\n",
    "    mycursor.execute('''\n",
    "    CREATE PROCEDURE UPDATE_NUM_CAMAS()\n",
    "        BEGIN   \n",
    "            CREATE TEMPORARY TABLE IF NOT EXISTS NUM_CAMAS_SALA (\n",
    "                            ID_HOSPITAL CHAR (6),\n",
    "                            ID_SALA CHAR (6),\n",
    "                            CAMAS_HOSPITAL INT,\n",
    "                            SALAS_HOSPITAL INT,\n",
    "                            CAMAS_SALA INT\n",
    "            );\n",
    "\n",
    "            SET SQL_SAFE_UPDATES = 0;\n",
    "            DELETE FROM NUM_CAMAS_SALA;\n",
    "            SET SQL_SAFE_UPDATES = 1;\n",
    "\n",
    "            INSERT INTO NUM_CAMAS_SALA (ID_HOSPITAL, ID_SALA, CAMAS_HOSPITAL, SALAS_HOSPITAL, CAMAS_SALA)\n",
    "                SELECT \n",
    "                    ASH.ID_HOSPITAL,\n",
    "                    ASH.ID_SALA,\n",
    "                    H.NUM_CAMAS_MAX,\n",
