    "sentencia_14 = pd.read_sql(\n",
    "    '''\n",
    "    SELECT \n",
    "        H.NOM_HOSPITAL, \n",
    "        SE.CATEGORIA_SALA, \n",
    "        NCS.CAMAS_HOSPITAL, \n",
    "        NCS.SALAS_HOSPITAL, \n",
    "        NCS.CAMAS_SALA,\n",
    "        COUNT(E.ID_CAMA) AS CAMAS_OCUPADAS,\n",
    "        ROUND(COUNT(E.ID_CAMA) / NCS.CAMAS_SALA, 2) AS OCUPACION_SALA,\n",
    "        ROUND(COUNT(E.ID_CAMA) / NCS.CAMAS_HOSPITAL, 2) AS OCUPACION_HOSPITAL\n",
    "    FROM ASIGNACION_SALAS_HOSPITALES ASH\n",
    "    INNER JOIN HOSPITALES H ON ASH.ID_HOSPITAL = H.ID_HOSPITAL\n",
    "    INNER JOIN SALAS_ESPECIALIDADES SE ON ASH.ID_SALA = SE.ID_SALA\n",
    "    INNER JOIN NUM_CAMAS_SALA NCS ON ASH.ID_HOSPITAL = NCS.ID_HOSPITAL AND ASH.ID_SALA = NCS.ID_SALA\n",
    "    INNER JOIN ENFERMOS E ON ASH.ID_HOSPITAL = E.ID_HOSPITAL AND ASH.ID_SALA = E.ID_SALA\n",
    "    GROUP BY ASH.ID_HOSPITAL, ASH.ID_SALA\n",
    "    ORDER BY OCUPACION_SALA DESC;\n",
    "    '''\n",
    "    , mydb)\n",
    "\n",
    "sentencia_14"
